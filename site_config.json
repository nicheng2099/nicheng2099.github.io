{
    "site_name": "DOC",
    "site_slogon": "笔记",
    "site_root_url": "/",
    "site_domain": "lc.study.io",
    "site_protocol": "https",
    "config_template_dir": "config",
    "route": {
        "docs": {
            "/doc1/": "docs/doc1",
            "/doc2/": "docs/doc2",
            "/Linux/": "docs/Linux",
            "/Odoo/": "docs/Odoo",
            "/Docker/": "docs/docker",
            "/Python/": "docs/python",
            "/Link/": "docs/Link"
        },
        "pages": {
            "/": "pages/index"
        },
        "assets": {
            "/static/": "static"
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "show_print_page": false,
                "heading_no_counter": true,
                "env":{
                    "main_color": "#CAAAA7"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "搜索"
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#CAAAA7",
                    "second_color": "#B0918D"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}